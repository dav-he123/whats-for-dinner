<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="stylesheet" href="/styles/styles.css" />
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;700&display=swap"
      rel="stylesheet"
    />
  </head>
  <body>
    <div class="header">
      <a href="#default"
        ><img src="/images/cookpot.svg" alt="Company Logo"
      /></a>

      <div class="header-right">
        <input class="addrecipe" type="button" value="Add Recipe" />
        <a href="/home">Home</a>
        <a href="#contact">Contact</a>
        <a href="#about">About</a>
      </div>
    </div>

    <section>
      <h1 class="favouriteRecipeHeader"> 
        Favourite Recipes:
      </h1>
      <div id="favrecipesdisplay">
        <% for(const element of favrecipe) { %>
        <p><%= element %></p>
        <% } %>
      </div>
    </section>

    <!-- Page-specific (main) content here -->
    <main class="container"></main>
  </body>
</html>
